{
    "name": "@name@",
    "version": "@version@",
    "manifest_version": 2,
    "description": "Classic New Tab Page is an extension the brings back the Classic Chrome New Tab Page.",
    "background": {
        "scripts": [ "js/analytics.js", "js/URI.min.js", "js/config.js", "js/background.js" ]
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "default_locale": "en",
    "icons": {
        "128": "images/icon/icon128.png",
        "16": "images/icon/icon16.png",
        "48": "images/icon/icon48.png"
    },
    @chrome-update-install-part@
    "permissions": [ "tabs", "http://*/*", "https://*/*", "chrome://favicon/", "management", "history", "bookmarks", "topSites", "unlimitedStorage", "storage" ],
    "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
